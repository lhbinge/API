isbn = {02649993},
issn = {02649993},
journal = {Economic Modelling},
keywords = {House prices,Monetary policy,Regime switching},
number = {1},
pages = {161--171},
title = {{Is the relationship between monetary policy and house prices asymmetric across bull and bear markets in South Africa? Evidence from a Markov-switching vector autoregressive model}},
volume = {32},
year = {2013}
}
