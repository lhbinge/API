@article{Burger2008,
abstract = {The question this paper investigates is whether or not different$\backslash$nmetropolitan areas each constitute a separate housing market or whether$\backslash$nor not there is a single South African housing market. Theory on the Law$\backslash$nof One Price suggests that if products or geographic areas belong in the$\backslash$nsame market, their absolute prices must converge, so that their relative$\backslash$nprices are stationary. By using cross-sectional time series data of five$\backslash$nmetropolitan areas, the paper tests for the Law of One Price by applying$\backslash$nthe Im, Pesaran and Shin panel unit root test. The paper finds strong$\backslash$nevidence of convergence in large middle-segment house prices and weaker$\backslash$nsupport for convergence in medium middle-segment house prices. In$\backslash$naddition, the paper finds no evidence for convergence in small$\backslash$nmiddle-segment house prices. This suggests the existence of a national$\backslash$nmarket for large and possibly middle-segment houses in metropolitan$\backslash$nareas, but separate metropolitan markets for small middle-segment$\backslash$nhouses. In addition, the paper estimates the speed of convergence and$\backslash$nfinds that large middle-segment house prices converge within two to$\backslash$nseven quarters, while the speed of convergence for medium middle-segment$\backslash$nhouse prices in three of the five areas is five to eight quarters.},
author = {Burger, Philippe and {Van Rensburg}, Lizelle Janse},
doi = {10.1111/j.1813-6982.2008.00190.x},
